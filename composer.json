{
    "name": "ockle/laravel-extasset",
    "description": "Cache external assets",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "illuminate/config": "^5.4",
        "illuminate/console": "^5.4",
        "illuminate/filesystem": "^5.4",
        "illuminate/log": "^5.4",
        "guzzlehttp/guzzle": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Ockle\\Extasset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ockle\\Extasset\\ServiceProvider"
            ],
            "aliases": {
                "Extasset": "Ockle\\Extasset\\Facade"
            }
        }
    }
}
